/* FORM BUTTONS */

.form-search-button { 
  margin-left: -5px;
  background: #0066A2;
  background: #1f45fc;  /* blue orchid */
  color: white;
  border-style: outset;
  border-color: #0066A2;
  border-radius: 5px;
  height: 30px;
  font: bold15px arial,sans-serif;
  text-shadow: none;
}
.form-search-button:hover {
  background: #0066A2;
  background: #0041c2;  /* blueberry blue */
  background: #1020c:d;  /* medium blue */
  color: #fff;
  border-radius: 5px;
  text-shadow: none;

  background-color: #9dc209;  /* pistachio green **1** */
}
.form-search-button:active {
  background: #342d7e;
  background: #0041c2;  /* blueberry blue */
  background: #0000b5;  /* new midnight blue */
  color: #fff;
  border: 1px solid #eee;
  border-radius: 5px;
  text-shadow: none;
}
.form-reset-button {
  border-style: outset;
  border-color: black;
  border-radius: 5px;
  height: 30px;
  font: bold15px arial,sans-serif;
  text-shadow: none;
}
.form-reset-button:hover {
  background: #016ABC;
  background: #d1doce;
  background: #e5e4e2;  /* platinum */
  background: #b6b6b4;  /* gray cloud */
  background: #d1d0c3;  /* gray goose */
  background: #c0c0c0;  /* silver */
  color: black;
  border-radius: 5px;
  text-shadow: none;
}
.form-reset-button:active {
  background: #b6b6b4;  /* gray cloud */
  background: #a9a9a9;  /* dark gray */
  background: #848482;  /* battleship gray */
  color: white;
  border: 1px solid #eee;
  border-radius: 5px;
  text-shadow: none;
}
.form-submit-button:hover {
  background: #016ABC;
  background: #0041c2;  /* blueberry blue */
  color: #fff;
  border: 1px solid black;
  border-radius: 5px;
  text-shadow: none;
}
.form-submit-button:active {
  background: #342d7e;  /* blue whale */
  background: #0041c2;  /* blueberry blue */
  color: #fff;
  border: 1px solid #eee;
  border-radius: 5px;
  text-shadow: none;
}
.form-submit-button {
  margin: 0;
  background: #0066A2;
  background: #1f45fc;  /* blue orchid */
  color: white;
  border-style: outset;
  border-color: #0066A2;
  border-radius: 5px;
  height: 30px;
  font: bold15px arial,sans-serif;
