/* PLACEMENTS */

.righthead {
  padding-right: 12%;
  text-align: right;
}
.middlehead {
  margin-left: 40%;
  padding-right: 15%;
  text-align: center;
}
span b {
  font-size: 80%;
  font-weight: normal;
}
ol {
  padding-left: 0%
  margin-left: 0px;
}
ul {
  padding: 0;
}
h2, title{
  display: inline-block;
}
title {
  padding: 0;
  margin: 0;
  left: 5px;
  position: relative;
  text-align: left;
  font-size: 20px;
}
h2 {
  text-align: left;
  font-size: 22px;
  padding: 0;
  margin: 0;
}


/* LINKS */

c {
  font-size: 26px;
  line-height: 26px;
}

d {
  font-size: 20px;
  line-height: 18px;
}

e {
  font-size: 16px;
}
a:link { text-decoration: none; }
a:visited { text-decoration: none; }
a:hover { text-decoration: underline; color: #ff6347; }  /* tomato */
a:active { text-decoration: underline; color: #342d7e; }
.nolink {
  text-decoration: none;
  color: black;
}
.uplink {
  text-decoration: none;
  color: black;
  font-size: 24px;
  font-weight: bold;
}
.twolink {
  text-decoration: none;
  color: black;
  font-size: 18px;
  font-weight: bold;
}
.smlink {
  text-decorated: none;
  color: black;
  font-size: 12px;
}
.remove {
  text-decoration: none;
  color: black;
  font-size: 12px;
  font-weight: bold;
}


